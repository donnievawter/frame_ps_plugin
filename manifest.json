{
  "id": "com.sangrephotography.add-frame-plugin",
  "name": "Add Frame",
  "version": "0.9.0",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "23.0.0"
    }
  ],
  "manifestVersion": 4,
  "entrypoints": [
    {
      "type": "panel",
      "id": "addframe",
      "minimumSize": {
        "width": 600,
        "height": 800
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width":600,
        "height": 800
      },
      "preferredFloatingSize": {
        "width": 600,
        "height": 800
      },
      "label": {
        "default": "Add Frame"
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "assets/icons/brownish23.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "assets/icons/brownish23.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "assets/icons/brownish48.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium",
        "lightest",
        "light",
        "all"
      ],
      "species": [
        "pluginList"
      ]
    }
  ]
}
